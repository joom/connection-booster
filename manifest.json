{
  "name": "Connection Booster",
  "description": "Find the optimal number of TCP connections.",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "webview",
    "unlimitedStorage",
    "notifications",
    "<all_urls>",
    "*://*/*",
    "http://*/*",
    "https://*/*"
  ],
  "sockets": {
          "tcp": {
              "connect": ""
          }
      },
  "icons": { "128": "booster.png" }
}
